.page5
    width: 100%;
    height: 100%;
    position: relative;
    .page5-background5
        z-index: 1
        width: 100%
        height: 100%
        position: absolute
    .page5-background-title
        z-index: 2
        width: 56%
        top: -16%
        left: 22%
        position: absolute
    .page5-background-title2
        top: 6%
        -webkit-animation:page5-background-title2-go 1s;
    .page5-shadow-container
        z-index: 2
        width: 88%
        height: 38%
        left: 6%
        top: -60%
        overflow: hidden
        position: absolute
        .page5-shadow-back
            width: 100%
            height: 100%
            position: absolute
        .page5-shadow-line
            width: 84%
            height: 100%;
            top: -100%;
            left: 8%
            position: absolute
        .page5-shadow-screen-container
            width: 92%
            height: 90%
            left: 4%
            top: 5%
            overflow: hidden
            position: absolute
            .page5-disease-msg
                z-index: 7
                width: 38%;
                height: 75%;
                top: 13%;
                right: 14%;
                position: absolute
            .page5-disease-msg-out
                top: -80%
                -webkit-animation:page5-disease-msg-out-go 1s;
            .page5-doctor
                z-index: 10
                width: 20%;
                height: 55%;
                bottom: 1.7%;
                left: 8%;
                position: absolute
            .page5-hand5
                z-index: 8
                width: 20%;
                height: 40%;
                left: 24%;
                bottom: 22%;
                position: absolute
            .page5-hand5-rotate
                transform: rotate(387deg);
                -webkit-transform: rotate(387deg);
                -moz-transform: rotate(387deg);
                -o-transform: rotate(387deg);
                -ms-transform: rotate(387deg);

                transform-origin: 10% 80%;
                -webkit-transform-origin: 10% 80%;
                -moz-transform-origin: 10% 80%;
                -o-transform-origin: 10% 80%;
                -ms-transform-origin: 10% 80%;
                -webkit-animation:page5-hand5-rotate-go 2s;
            .page5-image
                z-index: 7
                width: 52%
                height: 62%
                top: 100%;
                right: 7%;
                position: absolute
            .page5-image-in
                top: 20%
                -webkit-animation:page5-image-in-go 1s;
    .page5-shadow-container2
        top: 18%
        -webkit-animation:page5-shadow-container2-go 1s;
    .page5-expert1
        z-index: 11;
        width: 43%
        height: 36%
        left:-18%
        bottom: 0
        position: absolute
    .page5-expert2
        z-index: 11;
        width: 43%
        height: 36%
        left: 29%
        bottom: 0
        position: absolute
    .page5-expert3
        z-index: 11;
        width: 43%
        height: 36%
        left: 76%
        bottom: -36%
        position: absolute
    .page5-expert-in
        bottom: 0%
        -webkit-animation:page5-expert-in-go 1s;
    .page5-expert2-phone
        z-index: 12
        width: 43%
        height: 36%
        left: 29%
        bottom: 0
        position: absolute
    .page5-light
        z-index: 10
        width: 100%
        position: absolute
    .page5-progress-bar
        width: 80%
        top: 36%;
        left: 10%
        position: absolute
    .page5-title5
        z-index: 12;
        width: 64%
        left: 18%
        bottom: 5%
        position: absolute
@-webkit-keyframes page5-background-title2-go{
    0%{
        top: -16%;
    }
    100%{
        top: 6%;
    }
}
@-webkit-keyframes page5-expert-in-go{
    0%{
        bottom: -36%
    }
    100%{
        bottom: 0
    }
}
@-webkit-keyframes page5-disease-msg-out-go{
    0%{
       top: 13%
    }
    100%{
        top: -80%
    }
}
@-webkit-keyframes page5-image-in-go{
    0%{
       top: 100%
    }
    100%{
        top: 20%
    }
}
@-webkit-keyframes page5-hand5-rotate-go{
    0%{
        left: 24%;
        bottom: 22%;
    }
    100%{
        transform: rotate(387deg);
        -webkit-transform: rotate(387deg);
        -moz-transform: rotate(387deg);
        -o-transform: rotate(387deg);
        -ms-transform: rotate(387deg);

        transform-origin: 10% 80%;
        -webkit-transform-origin: 10% 80%;
        -moz-transform-origin: 10% 80%;
        -o-transform-origin: 10% 80%;
        -ms-transform-origin: 10% 80%;
    }
}
@-webkit-keyframes page5-shadow-container2-go{
    0%{
        top: -60%
    }
    100%{
        top: 18%
    }
}