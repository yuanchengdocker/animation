.page6
    width: 100%;
    height: 100%;
    position: relative;
    .page6-background
        width: 100%
        height: 100%
        position: absolute
    .page6-left-hand-container
        width: 100%
        height: 100%
        position: absolute
        .page6-screen-container
            z-index: 5
            top: 15.5%;
            left: 21%;
            width: 58%;
            height: 62%;
            overflow: hidden
            position: absolute
            .page6-flag
                z-index: 21;
                width: 100%
                height: 10%
                top: -10%
                position: absolute
            .page6-flag-in  
                top: 0;
                -webkit-animation:page6-flag-in-go 1s;
            .page6-order
                z-index: 22;
                top: 50%
                left: 50%
                width: 0%
                height: 0%
                position: absolute
            .page6-order-big-in
                top: 0
                left: 0
                width: 100%
                height: 100%
                -webkit-animation:page6-order-big-in-go 1s;
            .page6-image
                width: 250%;
                top: 0;
                left: -34%;
                height: 100%
                opacity: 0
                position: absolute
            .page6-image-in
                opacity: 1
                -webkit-animation:page6-image-in-go 3s;
            .page6-image-big
                width: 300%;
                left: -60%;
                top: -16%;
                height: 138%;
                -webkit-animation:page6-image-big-go 1s;
        .page6-left-hand
            z-index: 2
            width: 90%;
            height: 95%;
            top: 5%;
            left: -6%;
            position: absolute
        .page6-left-hand-pointer
            z-index: 6
            width: 20%
            height: 58%
            top: 26%
            left: 74%
            position: absolute
        .page6-screen-back
            z-index: 4
            top: 15.5%;
            left: 21%;
            width: 58%;
            height: 62%;
            position: absolute
    .page6-left-hand-container-out
        transform: rotate(340deg);
        -webkit-transform: rotate(340deg);
        -moz-transform: rotate(340deg);
        -o-transform: rotate(340deg);
        -ms-transform: rotate(340deg);
        left: -110%;
        bottom: -20%;
        -webkit-animation:page6-left-hand-container-out 1s;
    .page6-right-hand-container
        z-index: 7
        width: 100%
        height: 100%
        position: absolute
        .page6-right-hand-click
            width: 102%;
            height: 84%;
            bottom: -30%;
            right: -60%;
            position: absolute
        .page6-right-hand-click-out
            bottom: -85;
            right: -102%;
            -webkit-animation:page6-right-hand-click-out-go 1s;
        .page6-right-hand-click-move
            bottom: 0;
            right: -36%;
            -webkit-animation:page6-right-hand-click-move-go 1s;
        .page6-right-hand-click-click
            -webkit-animation:page6-right-hand-click-click-go 1s;
        .page6-right-hand-close
            width: 96%;
            height: 47%;
            bottom: -47%;
            right: -96%;
            position: absolute
        .page6-right-hand-close-in
            bottom: 0%;
            right: -20%;
            -webkit-animation:page6-right-hand-close-in-go 1s;    
        .page6-right-hand-open
            width: 100%;
            height: 50%;
            bottom: 0%;
            right: -20%;
            position: absolute
    .page6-progress-bar
        z-index: 10;
        width: 56.4%;
        top: 36%;
        left: 21.8%;
        position: absolute
    .page6-title
        width: 60%
        left: 20%
        bottom: 5%
        z-index: 23
        position: absolute
    .page6-title-out
        bottom: -10%
        -webkit-animation:page6-title-out-go 1s;
@-webkit-keyframes page6-image-in-go {
    0%{
        opacity: 0
    }
    100%{
        opacity: 1
    }
}
@-webkit-keyframes page6-title-out-go {
    0%{
        bottom: 5%
    }
    100%{
        bottom: -10%
    }
}
@-webkit-keyframes page6-left-hand-container-out {
    0%{
        transform: rotate(360deg);
        -webkit-transform: rotate(360deg);
        -moz-transform: rotate(360deg);
        -o-transform: rotate(360deg);
        -ms-transform: rotate(360deg);
        left: 0%;
        bottom: 0%;
    }
    100%{
        transform: rotate(340deg);
        -webkit-transform: rotate(340deg);
        -moz-transform: rotate(340deg);
        -o-transform: rotate(340deg);
        -ms-transform: rotate(340deg);
        left: -110%;
        bottom: -20%;
    }
}
@-webkit-keyframes page6-image-big-go {
    0%{
        width: 250%;
        top: 0;
        left: -34%;
        height: 100%
    }
    100%{
        width: 300%;
        left: -60%;
        top: -16%;
        height: 138%;
    }
}
@-webkit-keyframes page6-flag-in-go {
    0%{
        top: -10%;
    }
    100%{
        top: 0
    }
}
@-webkit-keyframes page6-right-hand-click-move-go {
    0%{
        bottom: -30%;
        right: -60%;
    }
    100%{
        bottom: 0;
        right: -36%;
    }
}
@-webkit-keyframes page6-right-hand-click-out-go {
    0%{
        bottom: 0;
        right: -36%;
    }
    100%{
        bottom: -85;
        right: -102%;
    }
}
@-webkit-keyframes page6-right-hand-click-click-go {
    0%{
        right: -36%;
    }
    50%{
        right: -34%;
    }
    100%{
        right: -36%;
    }
}
@-webkit-keyframes page6-order-big-in-go {
    0%{
        top: 50%
        left: 50%
        width: 0%
        height: 0%
    }
    100%{
        top: 0
        left: 0
        width: 100%
        height: 100%
    }
}
@-webkit-keyframes page6-right-hand-close-in-go {
    0%{
        bottom: -47%;
        right: -96%;
    }
    100%{
        bottom: 0%;
        right: -20%;
    }
}