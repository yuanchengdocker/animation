.page4
    width: 100%;
    height: 100%;
    background-color: #b28b73
    position: relative;
    .page4-container
        width: 100%;
        height: 100%;
        bottom: 100%
        position: absolute;
        .page4-background-far
            z-index: 1
            width: 100%
            height: 53%
            position: absolute
        .page4-background-near
            z-index: 3
            width: 100%
            height: 100%
            position: absolute
        .page4-dialog
            z-index: 8;
            width: 20%
            height: 8%
            left: 44%
            top: 26%
            position: absolute
        .page4-dialog2
            -webkit-animation:page4-dialog2-go 1s;
        .page4-service-container
            width: 100%;
            height: 100%;
            bottom: -70%;
            position: absolute;
            .page4-compute
                z-index: 8
                width: 40%
                height: 48%
                left: 0
                bottom: 7%
                position: absolute
            .page4-desk
                z-index: 7
                width: 56%
                height: 25%
                bottom: 0
                left: 0
                position: absolute
            .page4-hand
                z-index: 10
                width: 70%
                height: 10%
                left: 0
                bottom: 20%
                position: absolute
            .page4-hand2
                -webkit-animation:page4-hand2-go 1s;
            .page4-mouse1
                z-index: 9;
                left: 71.3%;
                top: 47%;
                width: 4.3%;
                position: absolute
            .page4-mouse2
                z-index: 9;
                left: 70.5%;
                top: 47%;
                width: 4.3%;
                position: absolute
            .page4-patient-msg
                z-index: 9;
                width: 24%;
                height: 31%;
                left: 3%;
                top: 35%;
                opacity: 0.5;
                position: absolute
            .page4-patient-msg2
                top: 46%;
                opacity: 1;
                -webkit-animation:page4-patient-msg2-go 1s;
            .page4-service
                z-index: 8
                width: 50%
                height: 70%
                bottom: 0
                right: 0
                position: absolute
        .page4-service-container2
            bottom: 0%;
            -webkit-animation:page4-service-container2-go 1s;
        .page4-title
            z-index: 9
            width: 60%
            left: 20%
            bottom: 5%
            position: absolute
        .page4-yun1
            z-index: 2
            width: 18%
            height: 9%
            top: 8%
            position: absolute
        .page4-yun2
            z-index: 2
            width: 14%
            height: 5%
            top: 4%
            position: absolute
    .page4-container2
        bottom: 0%
        -webkit-animation:page4-container2-go 1s;
@-webkit-keyframes page4-service-container2-go {
    0%{
        bottom: -70%
    }
    100%{
        bottom: 0%
    }
}
@-webkit-keyframes page4-container2-go {
    0%{
        bottom: 100%
    }
    100%{
        bottom: 0%
    }
}
@-webkit-keyframes page4-patient-msg2-go {
    0%{
        top: 35%;
        opacity: 0.5;
    }
    100%{
        top: 46%;
        opacity: 1;
    }
}
@-webkit-keyframes page4-dialog2-go{
    0%{
        width: 0
        height: 0
        left: 64%
        top: 34%
    }
    100%{
        width: 20%
        height: 8%
        left: 44%
        top: 26%       
    }
}
@-webkit-keyframes page4-hand2-go{
    0%{
        bottom: 20%;
    }
    50%{
        bottom: 19%;
    }
    100%{
        bottom: 20%;     
    }
}